    "codemirror": "^5.54.0",
    "prism-react-renderer": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel-bundler": "1.12.5",
    "pug": "^3.0.2",
    "sass": "^1.32.8"
